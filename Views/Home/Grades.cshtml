@model _413_1.Models.GradesModel

<body>
    <h3>Grade Calculator</h3>
    <!--Creates form on website to bring in input-->
    <form method="post">
        <label>Assignments:</label>
        <input id="assignments" asp-for="assignments"/>
        <br />
        <label>Group Project:</label>
        <input id="groupproject" asp-for="groupproject" />
        <br />
        <label>Quizzes:</label>
        <input id="quizzes" asp-for="quizzes"/>
        <br />
        <label>Exams:</label>
        <input id="exams" asp-for="exams"/>
        <br />
        <label>INTEX:</label>
        <input id="intex" asp-for="intex"/>
        <br />
        <br />
        <input type="submit" value="View Final Grade" id="submitButton" />
        <div asp-validation-summary="All"></div>    <!--brings up any errors-->
    </form>

    

</body>